{
  "name": "web-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "dev": "NODE_ENV=development PORT=3001 next dev",
    "lint": "prettier --check . && eslint . --ext .js,.jsx,.ts,.tsx && tsc --noEmit",
    "lint-fix": "prettier --write . && eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "validate": "yarn lint && yarn test:ci"
  },
  "dependencies": {
    "next": "12.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.1",
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "12.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "postcss": "^8.3.11",
    "prettier": "2.4.1",
    "tailwindcss": "^2.2.19",
    "typescript": "4.4.4"
  }
}
